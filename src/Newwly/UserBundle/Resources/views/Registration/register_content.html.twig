{% trans_default_domain 'FOSUserBundle' %}

<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="form-signin">
	<fieldset>
		<h3>{{ 'registration.submit'|trans }}</h3>
		{{ form_widget(form) }}
		<input type="submit" class="btn btn-primary btn-login" value="{{ 'registration.submit'|trans }}" />
	</fieldset>
        <a class="back-login" href="{{ path('fos_user_security_login') }}">{{ 'resetting.request.tologin'|trans }}</a>
</form>
